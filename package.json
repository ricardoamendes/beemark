{
  "name": "stock",
  "version": "0.0.0.1",
  "description": "A Chrome extension to drag and drop webpages in the favourites bookmark bar",
  "main": "index",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && NODE_ENV='development' babel src  --out-dir dist --copy-files",
    "dist": "npm run clean && NODE_ENV='production' webpack",
    "start": "NODE_ENV='development' webpack-dev-server --hot --inline"
  },
  "author": "Ricardo Mendes",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "json-loader": "^0.5.4",
    "webpack": "^2.6.1",
    "zip-webpack-plugin": "^1.1.0"
  }
}